{
  "name": "@strmprivacy-internal/api-definitions",
  "version": "2.5.0",
  "main": "build/index.ts",
  "files": [
    "/build",
    "/package.json"
  ],
  "publishConfig": {
    "@strmprivacy:registry": " https://europe-west4-npm.pkg.dev/stream-machine-development/npm/"
  },
  "devDependencies": {
    "@types/google-protobuf": "3.15.5",
    "@types/jquery": "3.3.38",
    "@types/node": "10.17.60",
    "barrelsby": "2.2.0",
    "ts-loader": "9.2.6",
    "typescript": "4.4.3",
    "webpack": "5.53.0",
    "webpack-cli": "4.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "google-protobuf": "~3.14.0",
    "grpc-web": "~1.2.1"
  },
  "scripts": {
    "get-version": "printf $npm_package_version",
    "authenticate": "cp .npmrc_template .npmrc && npx google-artifactregistry-auth .npmrc",
    "generate-barrels": "mkdir -p build && barrelsby --delete --directory build --exclude google --exclude internal_api",
    "build": "webpack --config webpack.config.js",
    "dist": "npm run generate-barrels && npm run build"
  }
}
